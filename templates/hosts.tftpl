%{ for group_name, group_config in groups ~}
[${group_name}]
%{ for instance_key, instance in group_config.instances ~}
${instance.name} ansible_host=${instance.network_interface.0.nat_ip_address} fqdn=${instance.fqdn}
%{ endfor ~}

%{ endfor ~}

[all:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/id_rsa